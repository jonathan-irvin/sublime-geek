        sysMessage(BASE,&quot;set&quot;,&quot;running&quot;);
        state default;
    }
    listen(integer channel,string name,key id,string msg){
        color = rgb2sl((vector)msg);        
        llOwnerSay(&quot;Setting Color...&quot;);        
        sysMessage(BASE,&quot;set&quot;,&quot;color|&quot;+(string)color);
        if(!liloact){
            sysMessage(BASE,&quot;set&quot;,&quot;running&quot;);
            state default;
            }
    }
    state_exit(){llSetTimerEvent(0);}
}


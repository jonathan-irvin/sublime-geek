<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings>
		
		<prev_button>
			<defaults round_corners="0,25,0,25" />
			<tweenIn x="20" tint="0x000000" alpha="0.2" />
			<tweenOver tint="0xFFFFFF" scaleX="1.05" scaleY="1.05"/>
			<tweenOut tint="0x000000" />
		</prev_button>
		
		<prev_symbol>
			<defaults type="2" />
			<tweenIn x="18" alpha="0.8" />
			<tweenOver tint="0x000000" />			
		</prev_symbol>
		
    	<next_button>
			<defaults round_corners="25,0,25,0" x="938" />			
			<tweenIn x="918" tint="0x000000" alpha="0.2" />
			<tweenOver tint="0xFFFFFF"  scaleX="1.05" scaleY="1.05"/>
			<tweenOut tint="0x000000" />
		</next_button>
		
		<next_symbol>
			<defaults type="2" />
			<tweenIn x="920" alpha="0.8" />
			<tweenOver tint="0x000000" />
		</next_symbol>
		
		<auto_play>
			<defaults symbol="circular" time="3" />
			<tweenIn x="900" y="38" width="18" height="18" tint="0xFFFFFF" />
		</auto_play>
		
			
	</settings>    

	<slides>
	
        <slide>
            <url>images/slideshow/slide-1.png</url>
        </slide>
		<!-- transition between slides -->
        <transition num="5" slicing="vertical" 	direction="down"  	delay=".05" duration="0.5"  z_multiplier="2"	/>
        <slide>
       		<url>images/slideshow/slide-2.png</url>
        </slide>		
		<!-- transition between slides -->
        <transition num="3" slicing="vertical"	direction="right" 	delay=".05" duration="0.5" 	z_multiplier="3"  	/>
		<slide>
            <url>images/slideshow/slide-3.png</url>
        </slide>
		<!-- transition between slides -->
        <transition num="5" slicing="vertical"	direction="left"  	delay=".05" duration="0.5" 	z_multiplier="3" 	/>
		<slide>
       		<url>images/slideshow/slide-4.png</url>
        </slide>
		
		<!-- transition between slides -->
		<transition num="3" slicing="vertical" 	direction="right" 	delay=".05" duration="0.5" 	z_multiplier="4" 	/>
        <slide>
       		<url>images/slideshow/slide-5.png</url>
        </slide>
		
		<!-- transition between slides -->
        <transition num="3" slicing="vertical" direction="down" 	delay=".05" duration="0.5" />
        <slide>
            <url>images/slideshow/slide-6.png</url>
        </slide>
		
		<!-- transition between slides 
        <transition num="3" slicing="horizontal" direction="right" delay="0.05" z_multiplier="4" />
        <slide>
       		<url>http://staging.sublimegeek.com/images/slideshow/slide-7.jpg</url>
        </slide>
		-->
		<!-- transition between slides 
        <transition num="3" slicing="vertical" direction="up" z_multiplier="3"/>
		<slide>
            <url>http://staging.sublimegeek.com/images/slideshow/slide-8.jpg</url>
        </slide>
		-->
				
	</slides>
</cu3er>

